{
  "kind": "collectionType",
  "collectionName": "gallery_images",
  "info": {
    "singularName": "gallery-image",
    "pluralName": "gallery-images",
    "displayName": "Gallery Image"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "image": {
      "type": "media",
      "multiple": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "caption": {
      "type": "text"
    },
    "sortOrder": {
      "type": "integer"
    },
    "gallery_category": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::gallery-category.gallery-category",
      "inversedBy": "gallery_images"
    }
  }
}
